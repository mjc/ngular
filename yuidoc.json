{
  "name": "The Ngular API",
  "description": "The Ngular API: a framework for building ambitious web applications",
  "url": "http://github.com/mjc/ngular/",
  "options": {
    "paths": [
      "packages/ngular/lib",
      "packages/ngular-debug/lib",
      "packages/ngular-metal/lib",
      "packages/ngular-runtime/lib",
      "packages/ngular-views/lib",
      "packages/ngular-htmlbars/lib",
      "packages/ngular-routing/lib",
      "packages/ngular-routing-htmlbars/lib",
      "packages/ngular-routing-views/lib",
      "packages/ngular-application/lib",
      "packages/ngular-testing/lib",
      "packages/ngular-extension-support/lib",
      "bower_components/rsvp/lib"
    ],
    "exclude": "vendor",
    "outdir":   "docs"
  }
}
